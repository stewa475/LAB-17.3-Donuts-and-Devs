export declare class SprkHeadingDirective {
    /**
     * @ignore
     */
    constructor();
    variant: 'displayOne' | 'displayTwo' | 'displayThree' | 'displayFour' | 'displayFive' | 'displaySix' | 'displaySeven';
    readonly headingOne: boolean;
    readonly headingTwo: boolean;
    readonly headingThree: boolean;
    readonly headingFour: boolean;
    readonly headingFive: boolean;
    readonly headingSix: boolean;
    readonly headingSeven: boolean;
    isPageTitle: boolean;
}
