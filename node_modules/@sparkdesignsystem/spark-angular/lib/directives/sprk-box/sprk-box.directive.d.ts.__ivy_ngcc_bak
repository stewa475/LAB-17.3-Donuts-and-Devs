export declare class SprkBoxDirective {
    /**
     * Padding inside of the box component.
     * Defaults to medium sizing when not provided.
     * Refer to Class Modifiers section for default pixel values.
     */
    spacing: 'flush' | 'tiny' | 'small' | 'medium' | 'large' | 'huge' | 'insetShortTiny' | 'insetShortSmall' | 'insetShortMedium' | 'insetShortLarge' | 'insetShortHuge' | 'insetTallTiny' | 'insetTallSmall' | 'insetTallMedium' | 'insetTallLarge' | 'insetTallHuge' | 'miscA' | 'miscB' | 'miscC' | 'miscD';
    true: any;
    readonly flush: boolean;
    readonly tiny: boolean;
    readonly small: boolean;
    readonly medium: boolean;
    readonly large: boolean;
    readonly huge: boolean;
    readonly insetShortTiny: boolean;
    readonly insetShortSmall: boolean;
    readonly insetShortMedium: boolean;
    readonly insetShortLarge: boolean;
    readonly insetShortHuge: boolean;
    readonly insetTallTiny: boolean;
    readonly insetTallSmall: boolean;
    readonly insetTallMedium: boolean;
    readonly insetTallLarge: boolean;
    readonly insetTallHuge: boolean;
    readonly miscA: boolean;
    readonly miscB: boolean;
    readonly miscC: boolean;
    readonly miscD: boolean;
    /**
     * @ignore
     */
    constructor();
}
